{
  "$schema": "https://raw.githubusercontent.com/martinring/tmlanguage/master/tmlanguage.json",
  "name": "asc",
  "fileTypes": ["asc"],
  "patterns": [
    {
      "include": "#armor-block"
    }
  ],
  "repository": {
    "armor-block": {
      "comment": "BLOCK BEGIN/END",
      "name": "constant.character.asc",
      "begin": "-{5}BEGIN (PGP MESSAGE|PGP PUBLIC KEY BLOCK|PGP PRIVATE KEY BLOCK|PGP MESSAGE, PART \\d+(|/\\d+)|PGP SIGNATURE)-{5}$",
      "end": "-{5}END \\1-{5}",
      "beginCaptures": {
        "0": {
          "name": "punctuation.paren.open.asc"
        }
      },
      "endCaptures": {
        "0": {
          "name": "punctuation.paren.close.asc"
        }
      },
      "patterns": []
    }
  },
  "scopeName": "source.asc"
}
