{
  "$schema": "https://raw.githubusercontent.com/martinring/tmlanguage/master/tmlanguage.json",
  "name": "sig",
  "fileTypes": ["sig"],
  "patterns": [
    {
      "include": "#armor-block"
    }
  ],
  "repository": {
    "armor-block": {
      "comment": "BEGIN/END PGP",
      "name": "constant.character.block.sig",
      "begin": "-{5}BEGIN PGP SIGNATURE-{5}$",
      "end": "-{5}END \\1-{5}",
      "patterns": []
    }
  },
  "scopeName": "source.sig"
}
