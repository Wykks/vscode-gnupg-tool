{
  "$schema": "https://raw.githubusercontent.com/martinring/tmlanguage/master/tmlanguage.json",
  "name": "gnupg_key",
  "fileTypes": ["key"],
  "patterns": [
    {
      "include": "#armor-block"
    }
  ],
  "repository": {
    "armor-block": {
      "comment": "BEGIN/END PGP",
      "name": "constant.character.block.gnupg_key",
      "begin": "-{5}(BEGIN (PGP PUBLIC KEY BLOCK|PGP PRIVATE KEY BLOCK))-{5}$",
      "beginCaptures": {
        "1": {"name": "constant.numeric.gnupg_key"}
      },
      "end": "-{5}(END \\2)-{5}",
      "endCaptures": {
        "1": {"name": "constant.numeric.gnupg_key"}
      }
    }
  },
  "scopeName": "source.gnupg_key"
}
