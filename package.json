{
  "name": "vscode-gnupg-tool",
  "repository": {
    "type": "git",
    "url": "https://github.com/heilingbrunner/vscode-gnupg-tool.git"
  },
  "displayName": "GnuPG-Tool",
  "description": "GnuPG tool",
  "version": "0.0.6",
  "publisher": "JHeilingbrunner",
  "license": "MIT",
  "icon": "images/vscode-gnupg-tool-logo.png",
  "engines": {
    "vscode": "^1.27.0"
  },
  "categories": [
    "Other"
  ],
  "keywords": [
    "encryption",
    "decryption",
    "gnupg",
    "gpg"
  ],
  "galleryBanner": {
    "color": "#1e1e1e",
    "theme": "dark"
  },
  "activationEvents": [
    "onCommand:extension.CheckGnuPG",
    "onCommand:extension.ListPublicKeys",
    "onCommand:extension.ListPrivateKeys",
    "onCommand:extension.ShowSmartcard",
    "onCommand:extension.EncryptSelection",
    "onCommand:extension.EncryptFile",
    "onCommand:extension.PreviewEncryptedFile",
    "onCommand:extension.DecryptSelection",
    "onCommand:extension.DecryptFile",
    "onCommand:extension.PreviewDecryptedFile",
    "onCommand:extension.SignFile",
    "onCommand:extension.VerifyFile",
    "onCommand:extension.EndSession"
  ],
  "contributes": {
    "commands": [
      {
        "command": "extension.CheckGnuPG",
        "title": "Check GnuPG",
        "category": "GnuPG"
      },
      {
        "command": "extension.ListPublicKeys",
        "title": "List Public Keys",
        "category": "GnuPG"
      },
      {
        "command": "extension.ListPrivateKeys",
        "title": "List Private Keys",
        "category": "GnuPG"
      },
      {
        "command": "extension.ShowSmartcard",
        "title": "Show Smartcard",
        "category": "GnuPG"
      },
      {
        "command": "extension.EndSession",
        "title": "End Session",
        "category": "GnuPG"
      },
      {
        "command": "extension.EncryptSelection",
        "title": "Encrypt Selection",
        "category": "GnuPG"
      },
      {
        "command": "extension.EncryptFile",
        "title": "Encrypt File",
        "category": "GnuPG"
      },
      {
        "command": "extension.PreviewEncryptedFile",
        "title": "Preview Encrypted File",
        "category": "GnuPG"
      },
      {
        "command": "extension.DecryptSelection",
        "title": "Decrypt Selection",
        "category": "GnuPG"
      },
      {
        "command": "extension.DecryptFile",
        "title": "Decrypt File",
        "category": "GnuPG"
      },
      {
        "command": "extension.PreviewDecryptedFile",
        "title": "Preview Decrypted File",
        "category": "GnuPG"
      },
      {
        "command": "extension.SignFile",
        "title": "Sign File",
        "category": "GnuPG"
      },
      {
        "command": "extension.VerifyFile",
        "title": "Verify File",
        "category": "GnuPG"
      }
    ],
    "menus": {
      "commandPalette": [
        {
          "command": "extension.CheckGnuPG",
          "group": "GnuPG"
        },
        {
          "command": "extension.ListPublicKeys",
          "group": "GnuPG"
        },
        {
          "command": "extension.ShowSmartcard",
          "group": "GnuPG"
        },
        {
          "command": "extension.EndSession",
          "group": "GnuPG"
        },
        {
          "command": "extension.EncryptFile",
          "group": "GnuPG"
        }
      ],
      "explorer/context": [
        {
          "command": "extension.PreviewEncryptedFile",
          "group": "gnupg1@1"
        },
        {
          "command": "extension.PreviewDecryptedFile",
          "group": "gnupg1@2"
        },
        {
          "command": "extension.EncryptFile",
          "group": "gnupg2@1"
        },
        {
          "command": "extension.DecryptFile",
          "group": "gnupg2@2"
        },
        {
          "command": "extension.SignFile",
          "group": "gnupg2@3"
        },
        {
          "command": "extension.VerifyFile",
          "group": "gnupg2@4"
        }
      ],
      "editor/context": [
        {
          "command": "extension.EncryptSelection",
          "when": "editorHasSelection",
          "group": "gnupg@1"
        },
        {
          "command": "extension.DecryptSelection",
          "when": "editorHasSelection",
          "group": "gnupg@2"
        }
      ],
      "editor/title": [
        {
          "command": "extension.EncryptFile"
        },
        {
          "command": "extension.DecryptFile"
        }
      ]
    },
    "configuration": {
      "type": "object",
      "title": "GnuPG Tool Configuration",
      "properties": {}
    }
  },
  "main": "./dist/extension",
  "scripts": {
    "vscode:prepublish": "webpack --mode production",
    "prev-vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "webpack": "webpack --mode development",
    "webpack-dev": "webpack --mode development --watch",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "npm run compile && node ./node_modules/vscode/bin/test"
  },
  "dependencies": {
    "gpg": "^0.6.0"
  },
  "devDependencies": {
    "typescript": "^2.6.1",
    "vscode": "^1.1.21",
    "tslint": "^5.8.0",
    "@types/node": "^8.10.25",
    "@types/mocha": "^2.2.42",
    "ts-loader": "^4.4.2",
    "webpack": "^4.19.1",
    "webpack-cli": "^3.1.0"
  }
}
